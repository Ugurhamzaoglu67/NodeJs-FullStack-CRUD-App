
<% alluser.forEach((user,index) => { %>
    <tr>
        <td><%= index+1 %> </td>
        <td><%= user.name %> </td>
        <td><%= user.email %> </td>
        <td><%= user.gender %> </td>
        <td><%= user.status %> </td>
        <td>
            <a href="/update-user" class="btn border-shadow update">
                <span class="text-gradien"><i class="fas fa-pencil-alt"></i></span>
            </a>
    
            <a class="btn border-shadow delete">
                <span class="text-gradien"><i class="fas fa-times"></i></span>
            </a>
           
        </td>
            
    </tr>
<% }) %>


